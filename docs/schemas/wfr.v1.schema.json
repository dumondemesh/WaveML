{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "WFR v1.x",
  "type": "object",
  "required": [
    "schema_version",
    "created_at",
    "run_id",
    "cert",
    "w_params",
    "w_perf",
    "metrics"
  ],
  "properties": {
    "schema_version": {
      "type": "string"
    },
    "created_at": {
      "type": "string"
    },
    "run_id": {
      "type": "string"
    },
    "cert": {
      "type": "object",
      "required": [
        "i1_unique_nf",
        "i2_delta_l_le_0",
        "i3_conservative_functors"
      ],
      "properties": {
        "i1_unique_nf": {
          "type": "boolean"
        },
        "i2_delta_l_le_0": {
          "type": "boolean"
        },
        "i3_conservative_functors": {
          "type": "boolean"
        }
      }
    },
    "w_params": {
      "type": "object",
      "required": [
        "n_fft",
        "hop",
        "window",
        "center",
        "pad_mode",
        "mode"
      ]
    },
    "w_perf": {
      "type": "object",
      "required": [
        "backend",
        "backend_version",
        "wall_ms",
        "frames"
      ]
    },
    "metrics": {
      "type": "object"
    }
  }
}