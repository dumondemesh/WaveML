echo "== Forge Gate: PASS =="
sIDA=$(id "$A");  echo "[forge-gate] NF-ID(A)   = NF-ID=$IDA"
IDB=$(id "$B");  echo "[forge-gate] NF-ID(B)   = NF-ID=$IDB"
if [[ "$IDA" != "$IDB" ]]; then
  echo "[forge-gate] FAIL: A and B must be identical after canon"
  "$W" nf-diff --left "$A" --right "$B" --show-source-diff || true
  exit 1
fi
echo "== Forge Gate: PASS =="
hIDA=$(id "$A");  echo "[forge-gate] NF-ID(A)   = NF-ID=$IDA"
IDB=$(id "$B");  echo "[forge-gate] NF-ID(B)   = NF-ID=$IDB"
if [[ "$IDA" != "$IDB" ]]; then
  echo "[forge-gate] FAIL: A and B must be identical after canon"
  "$W" nf-diff --left "$A" --right "$B" --show-source-diff || true
  exit 1
fi
echo "== Forge Gate: PASS =="

